<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/new_mf"
    tools:context=".In_game">

    <TableLayout
        android:id="@+id/defending"
        android:layout_width="200dp"
        android:layout_height="347dp"
        android:background="@color/Attacking_scoreboard_color_lighter"
        app:layout_constraintBottom_toTopOf="@+id/surrBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/attacking"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/Attacking_scoreboard_color_darker">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="200dp"
                android:layout_height="47dp">

                <TextView
                    android:id="@+id/defending_text"
                    android:layout_width="90dp"
                    android:layout_height="26dp"
                    android:fontFamily="@font/poppins_bold"
                    android:text="ATTACKING"
                    android:textColor="#FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.476" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="19dp" />
    </TableLayout>

    <TableLayout
        android:id="@+id/attacking"
        android:layout_width="200dp"
        android:layout_height="347dp"
        android:background="@color/defending_scoreboard_color_lighter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/defending_scoreboard_color_Darker">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="200dp"
                android:layout_height="47dp">

                <TextView
                    android:id="@+id/Attacking_Text"
                    android:layout_width="90dp"
                    android:layout_height="26dp"
                    android:fontFamily="@font/poppins_bold"
                    android:text="DEFENDING"
                    android:textColor="#FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.476" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/attacking_Text"
                android:layout_width="90dp"
                android:layout_height="26dp"
                android:fontFamily="@font/poppins_bold"
                android:text="DEFENDING"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.476" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </TableLayout>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView19"
        android:layout_width="434dp"
        android:layout_height="110dp"
        android:outlineSpotShadowColor="@color/black"
        android:scaleType="fitXY"
        android:src="@drawable/ascent_blur"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:shapeAppearanceOverlay="@style/Shape_for_map"
        app:strokeWidth="3dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/defender_scoreboard"
        android:layout_width="86dp"
        android:layout_height="74dp"
        android:layout_weight="1"
        android:background="@color/scoreboard"
        android:paddingLeft="10sp"
        android:paddingRight="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:shapeAppearanceOverlay="@style/ScoreBoards" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/attacker_scoreboard"
        android:layout_width="86dp"
        android:layout_height="74dp"
        android:layout_weight="1"
        android:background="@color/scoreboard"
        android:paddingLeft="10sp"
        android:paddingRight="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:shapeAppearanceOverlay="@style/ScoreBoards" />

    <TextView
        android:id="@+id/attacker_score"
        android:layout_width="68dp"
        android:layout_height="81dp"
        android:fontFamily="@font/poppins_bold"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="70sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.402" />

    <TextView
        android:id="@+id/defender_score"
        android:layout_width="68dp"
        android:layout_height="79dp"
        android:fontFamily="@font/poppins_bold"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="70sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.924"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.402" />

    <TextView
        android:id="@+id/Round"
        android:layout_width="18dp"
        android:layout_height="41dp"
        android:fontFamily="@font/poppins_bold"
        android:text=" 1"
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.641"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.956" />

    <TextView
        android:id="@+id/text_round"
        android:layout_width="102dp"
        android:layout_height="41dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Round "
        android:textAlignment="gravity"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.956" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/surrBtn"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="154dp"
        android:layout_height="49dp"
        android:layout_marginBottom="312dp"
        android:backgroundTint="@color/blackish"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text="SURRENDER"
        android:textColor="@color/Agent_selected_color"
        android:textSize="17sp"
        app:cornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:layout_constraintVertical_bias="1.0"
        app:strokeColor="@color/white"
        app:strokeWidth="2dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/Player_1_improved"
        android:layout_width="401dp"
        android:layout_height="112dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/transparent_white"
        android:contentDescription="TODO"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/surrBtn"
        app:layout_constraintVertical_bias="0.108"
        app:shapeAppearanceOverlay="@style/roundedImageView"
        app:strokeColor="#00E7FF" />

    <ScrollView
        android:id="@+id/kill_feed"
        android:layout_width="399dp"
        android:layout_height="109dp"
        android:paddingTop="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/Player_1_improved"
        app:layout_constraintEnd_toEndOf="@+id/Player_1_improved"
        app:layout_constraintStart_toStartOf="@+id/Player_1_improved"
        app:layout_constraintTop_toTopOf="@+id/Player_1_improved">

        <LinearLayout
            android:id="@+id/inner_kill_feed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingLeft="40dp"
            android:paddingRight="40dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="29dp"
                android:background="@drawable/redd"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compoundIconTextView12"
                    android:layout_width="152dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:background="@drawable/greenk"
                    android:drawableLeft="@drawable/k_breach"
                    android:drawableRight="@drawable/vandal"
                    android:gravity="center_vertical"
                    android:paddingRight="18dp"
                    android:text="bob"
                    android:textColor="#fff"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/compoundIconTextView22"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:drawableLeft="@drawable/wallbang"
                    android:drawableRight="@drawable/fbreachs"
                    android:gravity="center"
                    android:paddingLeft="1dp"
                    android:text="bob"
                    android:textColor="#fff"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="29dp"
                android:background="@drawable/redd"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/compoundIconTextView1"
                    android:layout_width="196dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0"
                    android:background="@drawable/greenk"
                    android:drawableLeft="@drawable/k_breach"
                    android:drawableRight="@drawable/vandal"
                    android:gravity="center_vertical"
                    android:paddingRight="18dp"
                    android:text="WatchFlankBro"
                    android:textColor="#fff"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/compoundIconTextView2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:drawableLeft="@drawable/wallbang"
                    android:drawableRight="@drawable/fbreachs"
                    android:gravity="center"
                    android:paddingLeft="1dp"
                    android:text="WatchFlankBro"
                    android:textColor="#fff"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:background="@drawable/empty"
                android:orientation="horizontal"></LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>

